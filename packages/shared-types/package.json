{
  "name": "@djed/shared-types",
  "version": "0.1.0",
  "description": "Common TypeScript types for LUXOR projects",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./mcp": {
      "types": "./dist/mcp.d.ts",
      "import": "./dist/mcp.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.js"
    },
    "./logging": {
      "types": "./dist/logging.d.ts",
      "import": "./dist/logging.js"
    },
    "./common": {
      "types": "./dist/common.d.ts",
      "import": "./dist/common.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "types",
    "typescript",
    "mcp",
    "luxor"
  ],
  "author": "LUXOR",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
